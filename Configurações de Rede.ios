=============================================================================================================================================================================
enable
    line vty 0 4
        login local 
        logging synchronous
        exec-timeout 5 30
        transport input ssh
        do write
        do show running-config
=============================================================================================================================================================================
!Configuração do SVI
enable
    configure terminal
        ip default-gateway 192.168.1.254
        interface vlan 1
        description Interface de Gerenciamento SVI
        ip address 192.168.1.250 255.255.255.0
        no shutdown
        end
    copy running-config startup-config
=============================================================================================================================================================================
!Configuração do SSH
enable
    configure terminal
        ip domain-name senac.br
        crypto key generate rsa general-keys modulus 1024
        ip ssh version 2
        ip ssh time-out 60
        ip ssh authentication-retries 2
        end
    copy running-config startup-config
    show running-config
    show ip interface brief
=============================================================================================================================================================================
!Prompt de Comando / CMD
C:\>ping 192.168.1.250
C:\>ssh
C:\>ssh -l admin 192.168.1.250
=============================================================================================================================================================================
!Configuração de 1 Link de Internet em dois Cabos
enable
    configure terminal
        hostname SW-A
        interface range gigabitEthernet 0/1-2
            channel-group 1 mode active
            end
    copy running-config startup-config
    show etherchannel
    show interfaces etherchannel
